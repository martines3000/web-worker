{
  "name": "web-worker",
  "version": "1.3.0",
  "description": "Consistent Web Workers in browser and Node.",
  "main": "./dist/node.js",
  "browser": "./dist/browser.js",
  "types": "./index.d.ts",
  "exports": {
    "import": {
      "types": "./index.d.mts",
      "bun": "./dist/browser.mjs",
      "browser": "./dist/browser.mjs",
      "default": "./dist/node.mjs"
    },
    "module": {
      "types": "./index.d.mts",
      "bun": "./dist/browser.mjs",
      "browser": "./dist/browser.mjs",
      "default": "./dist/node.mjs"
    },
    "default": {
      "types": "./index.d.ts",
      "bun": "./dist/browser.js",
      "browser": "./dist/browser.js",
      "default": "./dist/node.js"
    }
  },
  "files": [
    "dist",
    "browser.js",
    "node.js",
    "index.d.ts",
    "index.d.mts"
  ],
  "scripts": {
    "prepare": "babel node.js browser.js -d cjs",
    "test": "eslint '*.js' test && node --experimental-modules ./node_modules/.bin/ava"
  },
  "repository": "developit/web-worker",
  "keywords": [
    "worker",
    "worker_threads",
    "webworker",
    "web worker",
    "web-worker",
    "threads"
  ],
  "authors": [],
  "license": "Apache-2.0",
  "homepage": "https://github.com/developit/web-worker",
  "eslintConfig": {
    "extends": "developit",
    "rules": {
      "no-console": 0
    }
  },
  "devDependencies": {
    "ava": "2.4",
    "eslint": "^7.32.0",
    "eslint-config-developit": "^1.2.0",
    "eslint-plugin-compat": "^4.2.0"
  }
}
